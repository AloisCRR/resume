{
  "name": "alois-carrera-resume",
  "version": "1.0.0",
  "description": "Alois Carrera's professional resume in JSON Resume format, created and managed using jsonresume/resume-cli.",
  "main": "index.js",
  "scripts": {
    "start": "resume serve --theme stackoverflow",
    "start-es": "resume serve --theme stackoverflow --resume resume-es.json",
    "export": "resume export --theme stackoverflow",
    "export-pdf": "resume export resume.pdf --format pdf",
    "validate": "resume validate",
    "update-html": "node updateHtml.js",
    "build": "node scripts/build-resumes.js",
    "build:directus": "node scripts/build-resumes.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AloisCRR/resume.git"
  },
  "keywords": [
    "jsonresume",
    "resume",
    "career",
    "professional",
    "json"
  ],
  "author": "Alois Carrera",
  "license": "UNLICENSED",
  "dependencies": {
    "jsdom": "27.0.0",
    "jsonresume-theme-stackoverflow": "2.1.0",
    "resume-cli": "3.1.2"
  }
}
